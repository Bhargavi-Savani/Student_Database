<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Page3"
    android:background="@color/black">

    <ImageButton
        android:id="@+id/studentprofile"
        android:layout_width="380dp"
        android:layout_height="90dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="50dp"
        android:onClick="Student_profile"
        app:layout_constraintBottom_toTopOf="@+id/result"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.523"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/new_profile"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/result"
        android:layout_width="380dp"
        android:layout_height="100dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="50dp"
        android:onClick="Result"
        app:layout_constraintBottom_toTopOf="@+id/syllabus"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.523"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/studentprofile"
        app:srcCompat="@drawable/new_result"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/syllabus"
        android:layout_width="390dp"
        android:layout_height="90dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="150dp"
        android:onClick="Syllabus"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.588"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/result"
        app:layout_constraintVertical_bias="0.923"
        app:srcCompat="@drawable/new_syllabus"
        tools:ignore="SpeakableTextPresentCheck" />
</androidx.constraintlayout.widget.ConstraintLayout>